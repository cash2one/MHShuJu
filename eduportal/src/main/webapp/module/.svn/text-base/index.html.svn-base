<script type="text/javascript">
$(function(){
	srvMap.add('index_head', 'index_head.json','commonCMS-HomeTop.action?uid=cmsT');//头部区域
	srvMap.add('appList', 'noLoginApp.json','commonCMS-HomeBodyApps.action?uid=cmsBD');//应用展现
	srvMap.addLogin('changeHead', 'noLoginApp.json','user-SaveUserIcon.action?uid=saveIcon');//头像变更
	
	//生成登录后已经订购的产品跳转url
	//Handlebars.registerHelper('forwardUrl', function(sysType, nodeUid){
	//    return srvMap.getBaseUrl() + "edu_forward.jsp?nodeUid="+nodeUid+"&sysType="+sysType;
	//});
	
	var isLogin =loginUserInfo.rtnCode;//是否登录标志位  1：已登录
	//头部
	Util.ajax.postJson(srvMap.get('index_head'),'',function(json,status){
		if (status) {
			json.secmenus.hotFather = json.subMenu.father;
			json.secmenus.hotTeacher = json.subMenu.teacher;
			json.secmenus.hotActived = json.subMenu.actived;
			Util.ajax.loadTemp('#J_index_head',$('#T_index_head'),json);//菜单，广告轮播，快报
			$('#J_slides').smallslider();//广告轮播
			if (isLogin == '1') {
				$('.ui-name-role').text(loginUserInfo.bean.userName);//已登录则展示登录人姓名
				$('.head-icon,#J_show_head').attr('src',loginUserInfo.bean.IMGURL);//已登录则展示登录人头像
				if ($('.loginBtn').length>0) {
					$('.loginBtn').hide();
					$('.signinBtn').hide();
					$('.myOrderBtn').show();
					$('.myCollectBtn').show();
				}
				//更换头像
				$('.head-icon').bind('click', function(){
					art.dialog({
						title: "头像设置",
					    content: $('#J_changeHead').html(),
					    width: 550,
					    height: 430,
					    lock: true,
					    id: 'J_headIcon',
						padding: '10px 15px'
					});
				})
			}

			/*三级菜单的显示与与隐藏*/
			$('.ui-menus li').mouseover(function(){
				$(this).find('a.ui-menu-h3').addClass('cur').end().siblings().find('a.ui-menu-h3').removeClass('cur');
				$(this).find('.ui-third-menu').show().end().siblings().find('.ui-third-menu').hide();
			}).mouseout(function(){
				$(this).find('.ui-third-menu').hide();
				$(this).find('a.ui-menu-h3').removeClass('cur');
			})
			//鼠标滑过效果
			$('.ui-third-menu dl dd').hover(function(){
				$(this).addClass('hover');
			},function(){
				$(this).removeClass('hover');
			})
			//加载body部分
			var datas = [];
			if (isLogin == 1) {
				/*从cookie中取出我的足迹appID，传给后台，从后台获取对应业务信息*/
				var myTrack = $.cookie('myTrack');
				myTrack = JSON.parse(myTrack);
				for(var obj in myTrack){
					datas.push(myTrack[obj]['appID']);
				}
				datas = 'myTrack='+datas.join(',');
			}else{
				datas = '';
			}
			//获取应用列表---首页body部分
			Util.ajax.postJson(srvMap.get('appList'),datas,function(json,status){
				if (status) {
					if (isLogin == 1) {
						Util.ajax.loadTemp('#J_index_body',$('#T_login_app'),json);
					}else{
						Util.ajax.loadTemp('#J_index_body',$('#T_noLogin_app'),json);
					}
				}else{
					$('#J_index_body').html('<div class="ui-load-error"></div>')
				}
			})
		}else{
			$('#J_index_head').html('<div class="ui-load-error"></div>')
		}
	})
	if (!getCookie('noRemind')) {
		$('.ui-guide-index').removeClass('fn-hide');
		$('.ui-mask').removeClass('fn-hide');
	};
	//不再提示引导图片
	$('.ui-no-remind').click(function(){
		setCookie('noRemind','1',90);
		$('.ui-know').click();
	})
	//隐藏引导图片
	$('.ui-know').click(function(){
		$('.ui-guide-index').hide();
		$('.ui-mask').hide();
	})
})
function changeHead(obj){
	var imgSrc = $(obj).attr('src');
	var val = imgSrc.replace(/[^0-9]/ig,"");
	$('#J_head_confirm').attr('headVal',val);
	$('#J_show_head').attr('src','../theme/images/'+val+'.png');
}

//头像变更
function doChangeHead(obj){
	var imgName = $(obj).attr('headVal');
	Util.ajax.postJson(srvMap.get('changeHead'),"userIcon="+imgName,function(json,status){
		if (status) {
			art.dialog.tips('修改头像成功！');
			$('.head-icon').attr('src','../theme/images/'+imgName+'.png');
			art.dialog.list['J_headIcon'].close();
		}else{
			art.dialog.tips(json.rtnMsg||'修改头像失败，请重试！');
		}
	})
}
function cancelHead(){
	art.dialog.list['J_headIcon'].close();
}
</script>
</body>
</html>