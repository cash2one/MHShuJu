body,ul,li {
    padding: 0;
    margin: 0;
    border: 0;
}
body {
    overflow: hidden; /* this is important to prevent the whole page to bounce */
}
.fn-hide {
    display: none;
}
/* 清理浮动 */
.fn-clear:after {
    visibility:hidden;
    display:block;
    font-size:0;
    content:" ";
    clear:both;
    height:0;
}
.fn-clear {
    zoom:1; /* for IE6 IE7 */
}
#pullrefresh {
	background: #fff;
	padding-bottom: 50px;
}
/**
 * 聊天窗口
 * **/

.comui-wrap-ul {
    background: #fff;
    padding: 0 10px; 
	margin-top: 10px;
}
.comui-wrap-ul li {
    position: relative;
    clear: both;
    display: inline-block;
    padding: 0;
    margin: 0 0 20px 0;
    border-radius: 10px;
    font-size: 16px;
}
.comui-wrap-ul li .content-wrap {
    max-width: 70%;
    word-wrap:break-word;
	word-break: break-all;
}
.comui-wrap-ul li.left {
    float: left;
	color: #fff;
}
.comui-wrap-ul li.left .user_icon, .comui-wrap-ul li.left  .content-wrap {
	float: left;
}
.comui-wrap-ul li.left .user_icon:after {
	width: 50px;
	height: 50px;
	border-radius: 50px;
	display: inline-block;
	font-family: Muiicons;
	font-size: 54px;
	font-style: normal;
	font-weight: 400;
	line-height: 1;
	text-decoration: none;
	-webkit-font-smoothing: antialiased;
	content: '\e100';
	color: #184055;
}
.comui-wrap-ul li.left .content-wrap .user_name {
	margin: 4px 0 3px 15px;
	min-width: 160px;
}
.comui-wrap-ul li.left .content-wrap .conten {
	margin: 0 0 0 15px;
	position: relative;
	background-color: #184055;
	color: #fff;
	padding: 8px 12px;
	border-radius: 5px;
}
.comui-wrap-ul li.left .content-wrap .conten a {
	color: #fff;
}
.comui-wrap-ul li .content-wrap .conten a {
	text-decoration: underline;
}
.comui-wrap-ul li.left .content-wrap .conten:before {
	position: absolute;
    top: 12px;
    content: '';
    width: 0;
    height: 0;
    border-top: 10px solid #184055;
    border-left: 10px solid transparent;
    left: -10px;
}

.comui-wrap-ul li.right {
	float: right;
	color: #000;
}
.comui-wrap-ul li.right .user_icon, .comui-wrap-ul li.right  .content-wrap {
	float: right;
}
.comui-wrap-ul li.right .user_icon:after {
	width: 50px;
	height: 50px;
	border-radius: 50px;
	display: inline-block;
	font-family: Muiicons;
	font-size: 54px;
	font-style: normal;
	font-weight: 400;
	line-height: 1;
	text-decoration: none;
	-webkit-font-smoothing: antialiased;
	content: '\e100';
	color: #BABABA;
}
.comui-wrap-ul li.right  .content-wrap {
	margin-right: 10px;
}
.comui-wrap-ul li.right .content-wrap .user_name {
	margin: 4px 5px 3px 0;
	text-align: right;
	min-width: 160px;
}
.comui-wrap-ul li.right .content-wrap .conten {
	margin: 0 0 0 15px;
	position: relative;
	color: #000;
	padding: 8px 12px;
	border-radius: 5px;
	background: #EAEAE9;
}
.comui-wrap-ul li.right  .content-wrap .conten:before {
	position: absolute;
    top: 12px;
    content: '';
    width: 0;
    height: 0;
    border-top: 10px solid rgba(234, 234, 234, 1);
    border-right: 10px solid transparent;
    right: -10px;
}
.comui-wrap-ul li.left .content-wrap .conten .loading {
	position: absolute;
    right: -30px;
	top: 0;
    color: #000;
    font-size: 18px;
    -webkit-animation: myloading .5s infinite;
}

.comui-wrap-ul li.right .content-wrap .conten .loading {
	position: absolute;
    left: -30px;
	top: 0;
    color: #000;
    font-size: 18px;
    -webkit-animation: myloading .5s infinite;
}

