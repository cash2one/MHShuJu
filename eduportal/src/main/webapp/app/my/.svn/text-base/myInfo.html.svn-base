<script src="../lib/handlebars/1.3.0/handlebars.js"></script>
<script src="../common/config.js"></script>
<script src="../common/common.js"></script>
<script type="text/javascript">
$(function(){
	srvMap.add('getUserAllRole', 'choiceRole.json','commonOuterNormal.action?uid=getCurrentUserAllInfo');
	Util.ajax.postJson(srvMap.get('getUserAllRole'),'',function(json,status){
		if (status) {
			Util.ajax.loadTemp('#J_ListDiv',$('#T_ListDiv'),json);
			if(json.userInfo.userName!=null&&json.userInfo.userName.length>0){
			  var userInfoObj=json.userInfo;	
			  $('#J_imgUrl').attr('src',userInfoObj.imgUrl); 
			  $('#J_userName').text(userInfoObj.userName);
			  if(userInfoObj.sex=='1'){
				  $('#J_sex').text('男');
			  }else{
				  $('#J_sex').text('女');
			  }
			}
		}else{
			$('#J_ListDiv').html('<div class="ui-load-error">加载失败!</div>')
		}})
})
</script>
