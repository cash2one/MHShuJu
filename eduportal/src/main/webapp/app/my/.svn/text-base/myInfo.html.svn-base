<script id="T_ListDiv" type="text/x-handlebars-template">
	<ul class="mui-table-view" id="J_list">
		<li class="mui-table-view-cell mui-media">
         	<a class="mui-navigate-right">
	           	<img class="mui-media-object mui-pull-right" src="../theme/images/headIcon.png" id="J_imgUrl">
	           	<div class="mui-media-body" style="margin-top:16px;">
	             	头像
	           	</div>
         	</a>
      	</li>
      	<li class="mui-table-view-cell">
			<a class="mui-navigate-right">
				姓名
	           	<span class="mui-badge" id="J_userName">田亮</span>
			</a>
		</li>
      	<li class="mui-table-view-cell">
			<a class="mui-navigate-right">
				性别
	           	<span class="mui-badge" id="J_sex">男</span>
			</a>
		</li>
        {{#if parents.length}}
           <li class="mui-table-view-divider">角色信息 - 家长</li>
            {{#each parents}}
		     <li class="mui-table-view-cell mui-media">
         	   <a>
	           	   <img class="mui-media-object mui-pull-left" src="{{imgUrl}}">
	           	     <div class="mui-media-body">
	             	    {{userName}}
	             	    <p class='mui-ellipsis'>{{city}}{{region}}{{schoolName}}{{gradeName}}</p>
	           	     </div>
         	   </a>
      	      </li>
            {{/each}}
		{{/if}}
        {{#if teacher.length}}
            <li class="mui-table-view-divider">角色信息 - 老师</li>
            {{#each teacher}}
		    <li class="mui-table-view-cell mui-media">
         	   <a>
	              <img class="mui-media-object mui-pull-left" src="{{imgUrl}}">
	           	  <div class="mui-media-body">
	             	{{userName}}      
	             	<p class='mui-ellipsis'>{{city}}{{region}}{{schoolName}}{{gradeName}}</p>
	           	  </div>
         	  </a>
      	    </li>
           {{/each}}
		{{/if}}
	</ul>
</script>
<script src="../lib/jquery/2.1.3/jquery.min.js"></script>
